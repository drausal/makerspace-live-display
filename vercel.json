{
  "functions": {
    "app/api/cron/fetch-calendar/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/fetch-calendar",
      "schedule": "*/15 * * * *"
    }
  ],
  "env": {
    "CALENDAR_ID": "@calendar_id",
    "NEXT_PUBLIC_TIME_ZONE": "America/Chicago",
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  }
}
